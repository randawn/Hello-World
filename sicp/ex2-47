
(load "ex2-46")

(define (make-frame origin edge1 edge2)
    (list origin edge1 edge2)
)
(define (origin-frame frame)
    (car frame)
)
(define (edge1-frame frame)
    (cadr frame)
)
(define (edge2-frame frame)
    (caddr frame)
)

(define (make-frame origin edge1 edge2)
    (cons origin (cons edge1 edge2))
)
(define (origin-frame frame)
    (car frame)
)
(define (edge1-frame frame)
    (cadr frame)
)
(define (edge2-frame frame)
    (cddr frame)
)

(define frame
    (make-frame (make-vect 0 0)
                (make-vect 1 1)
                (make-vect 2 2)
    )
)
frame
(origin-frame frame)
(edge1-frame frame)
(edge2-frame frame)

